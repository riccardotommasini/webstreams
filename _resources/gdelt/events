@prefix docker: <https://www.linkeddata.stream/ontologies/docker> .

:events a vocals:StreamDescriptor ;
	dcat:dataset :eventstream  ;
        dcat:title "GDELT Events Stream"^^xsd:string ;
        dcat:publisher <http://www.linkeddata.stream/about> ;
        dcat:description "GDELT Events Stream"^^xsd:string ;
        dcat:license <https://creativecommons.org/licenses/by-nc/4.0/> ;

:eventstream a vocals:RDFStream ;
        vocals:windowType vocals:logicalTumbling ;
        vocals:windowSize "PT15M"^^xsd:duration ;
        vocals:hasEndpoint <ws://localhost:8080/events> .

<ws://localhost:8080/events>   a vocals:StreamEndpoint ;
                docker:image "streamreasoning/webstreams_gdelt_events"^^xsd:string ;
                docker:home "https://hub.docker.com/r/streamreasoning/webstreams_gdelt_events"^^xsd:string ;
                dcat:format frmt:JSON-LD;
                dcat:accessURL "ws://localhost:8080/events"^^xsd:string  .

